	"path_translator": [
		{ // convert HUD to game path
			"find": "UI/HUD/[a-zA-Z_]+/",
			"replace": "ui/uld/"
		},
		
		{ // convert Icon to game path
			"find": "UI/Icon/Icon/(\\d\\d\\d)\\d\\d\\d/",
			"replace": "ui/icon/${1}000/"
		}
		
		{ // make every folder in ui/uld hr1
			"find": "ui/uld/([a-zA-Z_])+/",
			"replace": "ui/uld/${1}_hr1/"
		}
	],
	
